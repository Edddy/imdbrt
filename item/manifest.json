{
  "name": "IMDBRT",
  "version": "3.5",
  "description": "Show Rotten Tomatoes/Flixster movie ratings on IMDB.",
  "icons": {
    "128" : "128_rotten_tomatoes.jpg"
  },
  "background" : {
    "scripts": ["background.js"]
  },
  "permissions": [
    "http://www.rottentomatoes.com/*",
    "http://rtapi.anacrolix.link/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.imdb.com/title/*/",
        "http://*.imdb.com/title/*/?*",
        "http://*.imdb.com/title/*/maindetails",
        "http://*.imdb.com/title/*/combined",
        "http://imdb.com/title/*/",
        "http://imdb.com/title/*/maindetails",
        "http://imdb.com/title/*/combined"
      ],
      "js" : ["jquery-1.4.3.js", "contentscript.js", "12897.js"]
    }
  ],
  "manifest_version": 2
}
